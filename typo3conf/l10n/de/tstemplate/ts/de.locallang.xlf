        <source>Constant Editor</source>
      <target state="translated">Konstanten-Editor</target></trans-unit>
      <trans-unit id="infoModify" xml:space="preserve" approved="yes">
        <source>Info/Modify</source>
      <target state="translated">Info/Bearbeiten</target></trans-unit>
      <trans-unit id="objectBrowser" xml:space="preserve" approved="yes">
        <source>TypoScript Object Browser</source>
      <target state="translated">TypoScript-Objekt-Browser</target></trans-unit>
      <trans-unit id="viewTemplates" xml:space="preserve" approved="yes">
        <source>Viewing templates of</source>
